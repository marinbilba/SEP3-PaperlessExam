@page "/ManageUsers"
@using SEP3.PaperlessExam.Data.PaperlessExamSevice
@inject NavigationManager NavigationManager
@inject IUserService UserService
<link rel="stylesheet" type="text/css" href="css/View/view.css">

<div class="container">
    <div class="row">
        <div class="col-md-5 red" @onclick="@ToCreateUser">
            <img src="images/add-group.png" style=" height: 120px;  align-content: center"/>
            <h4><b>Create User</b></h4></div>
        <div class="col-md-2"></div>
        <div class="col-md-5 red" @onclick="@ToFindEditDelete">
            <img src="images/edit.png" style=" height: 120px;  align-content: center"/>
            <h4><b>Find User</b></h4>
            <h4><b>Edit User</b></h4>
            <h4><b>Delete User</b></h4>
        </div>
    </div>
</div>

     @code {

    private void ToCreateUser()
    {
    NavigationManager.NavigateTo("/CreateUser");
    }
         private void ToFindEditDelete()
         {
             NavigationManager.NavigateTo("/FindEditDelete");
         }
         

     }